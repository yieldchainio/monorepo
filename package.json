{
  "name": "monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build-push": "node ./scripts/build-push-service.js"
  },
  "devDependencies": {
    "@types/node": "^20.1.7",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "ethers": "^6.5.1",
    "prisma": "^4.14.1",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@1.22.19"
}
